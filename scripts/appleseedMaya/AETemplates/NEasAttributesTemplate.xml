        <attribute name='pathRayIOR' type='maya.float'>
            <label>Path Ray IOR</label>
        </attribute>
        <attribute name='pathRayLength' type='maya.float'>
            <label>Path Ray Length</label>
        </attribute>
    </template>
    <view name='NEDefault' template='NEasAttributes'>
        <property name='assemblyInstanceId'/>
        <property name='assemblyInstanceName'/>
        <property name='assemblyName'/>
        <property name='cameraClipFar'/>
        <property name='cameraClipNear'/>
        <property name='cameraFOV'/>
        <property name='cameraPixelAspect'/>
        <property name='cameraProjection'/>
        <property name='cameraResolutionX'/>
        <property name='cameraResolutionY'/>
        <property name='cameraScreenWindowXMax'/>
        <property name='cameraScreenWindowXMin'/>
        <property name='cameraScreenWindowYMax'/>
        <property name='cameraScreenWindowYMin'/>
        <property name='cameraShutterClose'/>
        <property name='cameraShutterOpen'/>
        <property name='objectInstanceId'/>
        <property name='objectInstanceIndex'/>
        <property name='objectInstanceName'/>
        <property name='objectName'/>
        <property name='pathHasRayDifferentials'/>
        <property name='pathRayDepth'/>
        <property name='pathRayIOR'/>
        <property name='pathRayLength'/>
