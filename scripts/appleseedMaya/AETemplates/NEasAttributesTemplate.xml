<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEasAttributes'>
        <attribute name='assemblyInstanceId' type='maya.long'>
            <label>Assembly Instance Id</label>
        </attribute>
        <attribute name='assemblyInstanceName' type='maya.string'>
            <label>Assembly Instance Name</label>
        </attribute>
        <attribute name='assemblyName' type='maya.string'>
            <label>Assembly Name</label>
        </attribute>
        <attribute name='cameraClipFar' type='maya.float'>
            <label>Camera Clip Far</label>
        </attribute>
        <attribute name='cameraClipNear' type='maya.float'>
            <label>Camera Clip Near</label>
        </attribute>
        <attribute name='cameraFOV' type='maya.float'>
            <label>Camera FOV</label>
        </attribute>
        <attribute name='cameraPixelAspect' type='maya.float'>
            <label>Camera Pixel Aspect</label>
        </attribute>
        <attribute name='cameraProjection' type='maya.string'>
            <label>Camera Projection</label>
        </attribute>
        <attribute name='cameraResolutionX' type='maya.long'>
            <label>Camera Resolution X</label>
        </attribute>
        <attribute name='cameraResolutionY' type='maya.long'>
            <label>Camera Resolution Y</label>
        </attribute>
        <attribute name='cameraScreenWindowXMax' type='maya.float'>
            <label>Camera Screen Window XMax</label>
        </attribute>
        <attribute name='cameraScreenWindowXMin' type='maya.float'>
            <label>Camera Screen Window XMin</label>
        </attribute>
        <attribute name='cameraScreenWindowYMax' type='maya.float'>
            <label>Camera Screen Window YMax</label>
        </attribute>
        <attribute name='cameraScreenWindowYMin' type='maya.float'>
            <label>Camera Screen Window YMin</label>
        </attribute>
        <attribute name='cameraShutterClose' type='maya.float'>
