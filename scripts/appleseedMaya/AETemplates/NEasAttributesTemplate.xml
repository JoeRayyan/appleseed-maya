        </attribute>
        <attribute name='objectInstanceId' type='maya.long'>
            <label>Object Instance Id</label>
        </attribute>
        <attribute name='objectInstanceIndex' type='maya.long'>
            <label>Object Instance Index</label>
        </attribute>
        <attribute name='objectInstanceName' type='maya.string'>
            <label>Object Instance Name</label>
        </attribute>
        <attribute name='objectName' type='maya.string'>
            <label>Object Name</label>
        </attribute>
        <attribute name='pathHasRayDifferentials' type='maya.long'>
            <label>Path Has Ray Differentials</label>
        </attribute>
        <attribute name='pathRayDepth' type='maya.long'>
            <label>Path Ray Depth</label>
