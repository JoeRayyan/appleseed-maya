<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEasDoubleShade'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='color' type='maya.float3'>
			<label>Color</label>
		</attribute>
		<attribute name='color2' type='maya.float3'>
			<label>Color 2</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEasDoubleShade'>
		<property name='outColor'/>
		<property name='color'/>
		<property name='color2'/>
	</view>
</templates>
