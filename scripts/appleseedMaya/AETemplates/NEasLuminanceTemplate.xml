<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEasLuminance'>
        <attribute name='result' type='maya.float'>
            <label>Result</label>
        </attribute>
        <attribute name='color' type='maya.float3'>
            <label>Color</label>
        </attribute>
        <attribute name='deriveFromMayaCMS' type='maya.bool'>
            <label>Derive From Maya CMS</label>
        </attribute>
        <attribute name='inputColorSpace' type='maya.enum'>
            <label>Input Color Space</label>
        </attribute>
        <attribute name='chromaticityCoordsR' type='maya.float2'>
            <label>Chromaticity Coords R</label>
        </attribute>
        <attribute name='chromaticityCoordsG' type='maya.float2'>
            <label>Chromaticity Coords G</label>
        </attribute>
        <attribute name='chromaticityCoordsB' type='maya.float2'>
            <label>Chromaticity Coords B</label>
        </attribute>
        <attribute name='chromaticityCoordsW' type='maya.float2'>
            <label>Chromaticity Coords W</label>
        </attribute>
        <attribute name='inputIlluminants' type='maya.enum'>
            <label>Input Illuminants</label>
        </attribute>
        <attribute name='colorTemperature' type='maya.long'>
            <label>Color Temperature</label>
        </attribute>
    </template>
    <view name='NEDefault' template='NEasLuminance'>
        <property name='result'/>
        <property name='color'/>
        <property name='deriveFromMayaCMS'/>
        <property name='inputColorSpace'/>
        <property name='chromaticityCoordsR'/>
        <property name='chromaticityCoordsG'/>
        <property name='chromaticityCoordsB'/>
        <property name='chromaticityCoordsW'/>
        <property name='colorTemperature'/>
        <property name='inputIlluminants'/>
    </view>
</templates>
