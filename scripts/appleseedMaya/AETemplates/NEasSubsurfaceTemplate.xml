<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEasSubsurface'>
        <attribute name='outColor' type='maya.float3'>
            <label>Out Color</label>
        </attribute>
        <attribute name='outMatteOpacity' type='maya.float3'>
            <label>Out Matte Opacity</label>
        </attribute>                
        <attribute name='color' type='maya.float3'>
            <label>Color</label>
        </attribute>
        <attribute name='sssAmount' type='maya.float'>
            <label>Sss Amount</label>
        </attribute>
        <attribute name='meanFreePath' type='maya.float3'>
            <label>Mean Free Path</label>
        </attribute>
        <attribute name='mfpScale' type='maya.float'>
            <label>Mfp Scale</label>
        </attribute>
        <attribute name='ior' type='maya.float'>
            <label>Ior</label>
        </attribute>
        <attribute name='fresnelWeight' type='maya.float'>
            <label>Fresnel Weight</label>
        </attribute>
        <attribute name='specularWeight' type='maya.float'>
            <label>Specular Weight</label>
        </attribute>
        <attribute name='specularRoughness' type='maya.float'>
            <label>Specular Roughness</label>
        </attribute>
        <attribute name='anisotropyAmount' type='maya.float'>
            <label>Anisotropy Amount</label>
        </attribute>
        <attribute name='anisotropyAngle' type='maya.float'>
            <label>Anisotropy Angle</label>
        </attribute>
        <attribute name='anisotropyMap' type='maya.float3'>
            <label>Anisotropy Map</label>
        </attribute>
        <attribute name='anisotropyDirection' type='maya.float3'>
            <label>Anisotropy Direction</label>
        </attribute>
        <attribute name='normalCamera' type='maya.float3'>
            <label>Normal Camera</label>
        </attribute>
        <attribute name='matteOpacity' type='maya.float'>
            <label>Matte Opacity</label>
        </attribute>
        <attribute name='matteOpacityColor' type='maya.float3'>
            <label>Matte Opacity Color</label>
        </attribute>
    </template>
    <view name='NEDefault' template='NEasSubsurface'>
        <property name='outColor'/>
        <property name='outMatteOpacity'/>
        <property name='color'/>
        <property name='sssAmount'/>
        <property name='meanFreePath'/>
        <property name='mfpScale'/>
        <property name='ior'/>
        <property name='fresnelWeight'/>
        <property name='specularWeight'/>
        <property name='specularRoughness'/>
        <property name='anisotropyAmount'/>
        <property name='anisotropyAngle'/>
        <property name='anisotropyMap'/>
        <property name='anisotropyDirection'/>
        <property name='normalCamera'/>
        <property name='matteOpacity'/>
        <property name='matteOpacityColor'/>
    </view>
</templates>
